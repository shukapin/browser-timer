{
  "name": "Timer",
  "description": "Timer",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["alarms", "notifications", "storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Timer",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "timer-icon.png",
    "32": "timer-icon.png",
    "48": "timer-icon.png",
    "128": "timer-icon.png"
  }
}
